<%- include('includes/header') %>
  
  <div class="container py-md-5 container--narrow">
    <%- include('includes/flash') %>
    
    <%- include('includes/sharedProfile') %>

    
  
    <div class="list-group">

      <% following.forEach(element =>{ %>
          <a href="/profile/<%= element.username %>" class="list-group-item list-group-item-action">
          <img class="avatar-tiny" src="<%= element.avatar %>">
